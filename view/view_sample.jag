<h2> Sample dashboard</h2>

<p>&nbsp;</p>

                  <%
var req_data = request.getParameter("data");
id = parse(decodeURIComponent(req_data));

include("../controller/base64_encode.jag");
var SERVICE_SECURE_ROOT = session.get("service_secure_root");
var url  = SERVICE_SECURE_ROOT+"WSO2_QAP_SAMPLE_DataService/view/samples_with_id/"+id;


var optionalHeaders = {
    "Accept": "application/json",
    "Authorization": "Basic "+base64_encode("admin:admin") };

var param="";

var data = get(url,"",optionalHeaders);


var obj = parse(data.data);

var json_data=obj.WSO2_QAP_SAMPLECollection.WSO2_QAP_SAMPLE;

if(json_data.WSO2_QAP_SAMPLE_ID===null || json_data.WSO2_QAP_SAMPLE_NAME===null){
    print("Sorry this page isn't available");
}else{
    print("<h2>");
    print(json_data.WSO2_QAP_SAMPLE_NAME);
    print("</h2>");
}

%>


<div id="workArea">

   <form  name="featureForm" method="post">
      <input name="pageNumber" value="0" type="hidden">
      <input name="webappState" value="all" type="hidden">
      <input name="webappType" value="all" type="hidden">
      <table class="styledLeft" id="webappTable" style="margin-left: 0px;"
                           width="100 ">

         <thead>
          <tr class="tableEvenRow">
            <th colspan="2" align="left"><strong><%= json_data.WSO2_QAP_SAMPLE_NAME%></strong></th>

          </tr>
          </thead>

