
<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns="http://www.w3.org/1999/xhtml">
   <head>
      <%
         var name = session.get("username");
         include("template/head.jag");
         %>
   </head>
   <body>

      <div id="dcontainer"></div>
      <script type="text/javascript" src="../resources/js/dialog.js"></script>
      <!-- JS imports for collapsible menu -->
      <script src="../resources/js/yahoo-dom-event.js" type="text/javascript"></script>
      <script src="../resources/js/animation-min.js" type="text/javascript"></script>
      <script src="../resources/js/template.js" type="text/javascript"></script>
      <script src="../resources/js/yahoo-min.js" type="text/javascript"></script>
      <script src="../resources/js/selector-min.js" type="text/javascript"></script>

      <table id="main-table" border="0" cellspacing="0">
         <tbody>
            <tr>
               <td id="header" colspan="3">
                  <%
                     require("template/header_div.jag");
                     %>
               </td>
            </tr>
            <tr id="middle-container-tr">
               <%
                  require("template/product_vertical_menu.jag");
                  %>
               <%
                  require("template/product_menu_panel.jag");
                  %>
               <td id="middle-content">
                  <table id="content-table" border="0" cellspacing="0">
                     <tbody>
                        <tr>
                           <td id="page-header-links">
                              <table class="page-header-links-table" cellspacing="0">
                                 <tbody>
                                    <tr>
                                       <td class="breadcrumbs">
                                          <table class="breadcrumb-table" cellspacing="0">
                                             <tbody>
                                                <tr>
                                                   <td>
                                                      <div id="breadcrumb-div">
                                                         
                                                      </div>
                                                   </td>
                                                </tr>
                                             </tbody>
                                          </table>
                                       </td>
                                       <td class="page-header-help"><a href="./docs/userguide.html" target="_blank">Help</a></td>
                                    </tr>
                                 </tbody>
                              </table>
                           </td>
                        </tr>
                        <tr>
                           <td id="body">
                              <img src="../resources/images/1px.gif" width="735px" height="1px">
                              <script type="text/javascript">
                                 setCookie('current-breadcrumb', 'manage_menu,services_menu,services_add_menu,ds_menu,ds_generate_menu#');
                                 
                              </script>
                              <script type="text/javascript" src="../resources/js/ui_validator.js"></script>
                              <div id="middle">
                              <%

                                 data=request.getParameter("data");
                                 if(!(data == null)){
                                    obj=parse(data);
                                    obj2=parse(obj);
                                    WSO2_QAP_TEST_SCENARIO_NAME = obj2.WSO2_QAP_TEST_SCENARIO_NAME;
                                    WSO2_QAP_TEST_SCENARIO_ID = obj2.WSO2_QAP_TEST_SCENARIO_ID;
                                    WSO2_QAP_DESCRIPTION=obj2.WSO2_QAP_DESCRIPTION;
                                 }
                                 if(!(data == null)){
                                    print("<h2>");
                                    print("Edit Test Scenario");
                                    print("</h2>");
                                 }else{
                                    print("<h2>");
                                    print("Add Test Scenario");
                                    print("</h2>");
                                 }
                                 %>
                                 
                                 <div id="workArea">
                                 <!--  -->
                                    <form name="add_testscenario" action="../controller/addTestScenario.jag" method="post" onsubmit="return validateTextInput();return false;">
                                       <table class="styledLeft">
                                          <thead>
                                             <tr>
                                                <th colspan="2">Test Scenario</th>
                                             </tr>
                                          </thead>
                                          <tbody>
                                             <tr>
                                                <td>
                                                
                                                   <table class="normal">
                                                      <tbody>
                                                         <tr>
                                                            <td>Scenario Name<font color="red">*</font>
                                                            </td>
                                                            <%
                                                               if(!(data == null)){
                                                                  print("<td>");
                                                                  print('<input size="35" class="text-not-empty" name="WSO2_QAP_TEST_SCENARIO_NAME" class="text-not-empty" id="WSO2_QAP_TEST_SCENARIO_NAME" value="'+WSO2_QAP_TEST_SCENARIO_NAME+'" type="text">');
                                                                  print("</td>");
                                                               }
                                                            else{
																//print("ok");
                                                               print("<td>");
                                                                  print('<input size="35" class="text-not-empty" name="WSO2_QAP_TEST_SCENARIO_NAME" class="text-not-empty" id="WSO2_QAP_TEST_SCENARIO_NAME" value="" type="text">');
                                                                  print("</td>");
                                                            }
                                                            %>
                                                         </tr>
                                                         <tr>
                                                            
                                                            <td>Description<font color="red">*</font>
                                                            </td>
                                                            <%
                                                               if(!(data == null)){
                                                                  print("<td>");
                                                                  print('<input size="35" class="text-not-empty" name="WSO2_QAP_DESCRIPTION" class="text-not-empty" id="WSO2_QAP_DESCRIPTION" value="'+WSO2_QAP_DESCRIPTION+'" type="text">');
                                                                  print("</td>");
                                                               }
                                                            else{
                                                               print("<td>");
                                                                  print('<input size="35" class="text-not-empty" name="WSO2_QAP_DESCRIPTION" class="text-not-empty" id="WSO2_QAP_DESCRIPTION" value="" type="text">');
                                                                  print("</td>");
                                                            }
                                                            %>
                                                            
                                                         </tr>
                                                         <tr>
                                                            <td class="buttonRow">
                                                               <input class="button" value="Save" type="submit">
                                                               <input class="button" value="Cancel" onclick="location.href = 'testscenario_home.jag'" type="button">
                                                            </td>
                                                         </tr>
                                                      </tbody>
                                                   </table>

                                                </td>
                                             </tr>
                                          </tbody>
                                       </table>
                                       <%
                                          if(!(data == null)){
                                             print('<input type="hidden" name="WSO2_QAP_TEST_SCENARIO_ID" value="'+WSO2_QAP_TEST_SCENARIO_ID+'">');
                                          }
                                       %>
                                    </form>
                                 </div>
                              </div>
                           </td>
                        </tr>
                     </tbody>
                  </table>
               </td>
            </tr>
            <tr>
               <%
                  include("template/footer.jag")
                  %>
            </tr>
         </tbody>
      </table>
   </body>
</html>